<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Enrollment_Date_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>ISNEW() &amp;&amp;  Enrollment_Date__c &lt; TODAY()</errorConditionFormula>
