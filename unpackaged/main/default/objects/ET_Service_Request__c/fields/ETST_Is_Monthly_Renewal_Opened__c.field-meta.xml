<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ETST_Is_Monthly_Renewal_Opened__c</fullName>
    <externalId>false</externalId>
    <formula>((ETST_Pick_Up_End_Date__c - TODAY() )&lt;=30 ) &amp;&amp;  (TEXT(ETST_Transport_Type__c) &lt;&gt; &#39;Yearly&#39; )
