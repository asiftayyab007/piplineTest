<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Inspector4Validation</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
NOT(ISBLANK(Inspector_4__c)),
NOT(REGEX(Inspector_4__c , &quot;^[0-9.]+$&quot;))
)</errorConditionFormula>
    <errorDisplayField>Inspector_4__c</errorDisplayField>
    <errorMessage>Enter numbers only</errorMessage>
</ValidationRule>
