<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ScheduleEndDateValidation</fullName>
    <active>true</active>
    <errorConditionFormula>Scheduled_Start__c &gt; Scheduled_End__c</errorConditionFormula>
    <errorDisplayField>Scheduled_End__c</errorDisplayField>
    <errorMessage>Schedule end date/time should be greater than start date/time</errorMessage>
</ValidationRule>
