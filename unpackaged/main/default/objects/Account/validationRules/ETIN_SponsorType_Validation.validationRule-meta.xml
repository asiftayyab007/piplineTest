<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ETIN_SponsorType_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
NOT(ISBLANK( ETIN_Sponsor_UID_Type__c )),
NOT(REGEX( ETIN_Sponsor_UID_Type__c , &quot;^[0-9A-Za-z]+$&quot;))
)</errorConditionFormula>
    <errorDisplayField>ETIN_Sponsor_UID_Type__c</errorDisplayField>
    <errorMessage>Please enter valid characters</errorMessage>
</ValidationRule>
