<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ETIN_employeeID_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
NOT(ISBLANK(ETIN_Employee_Id__c )),
NOT(REGEX(ETIN_Employee_Id__c  , &quot;^[0-9.]+$&quot;))
)</errorConditionFormula>
    <errorDisplayField>ETIN_Employee_Id__c</errorDisplayField>
    <errorMessage>Please enter only numbers</errorMessage>
</ValidationRule>
