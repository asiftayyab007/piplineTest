<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ETST_Parent_EID_Validation</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
NOT(ISBLANK(ET_Emirates_Id__c )),
NOT(REGEX(ET_Emirates_Id__c ,&quot;([784-]{4}[0-9-]{5}[0-9-]{8}[0-9]{1})&quot;))
)</errorConditionFormula>
    <errorDisplayField>ET_Emirates_Id__c</errorDisplayField>
    <errorMessage>Please enter Emirates Id in the correct format (784-XXXX-XXXXXXX-X)</errorMessage>
</ValidationRule>
