<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ET_Inactive_Quote_Cannot_be_Accpeted</fullName>
    <active>true</active>
    <errorConditionFormula>IF( !IsActive__c &amp;&amp; ISPICKVAL(Status , &#39;Accepted&#39;), true, false )</errorConditionFormula>
    <errorMessage>if quote is in active you can not mark Quote as accepted.</errorMessage>
</ValidationRule>
