<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Other_Industry_Validation_Rule</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ISPICKVAL(Industry, &#39;Other&#39;),
ISBLANK( ETSALES_Other_Industry__c )
)</errorConditionFormula>
    <errorDisplayField>ETSALES_Other_Industry__c</errorDisplayField>
    <errorMessage>Please mention other Industry name</errorMessage>
</ValidationRule>
