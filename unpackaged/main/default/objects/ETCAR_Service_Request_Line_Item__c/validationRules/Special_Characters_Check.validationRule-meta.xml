<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Special_Characters_Check</fullName>
    <active>false</active>
    <errorConditionFormula>OR(
NOT(REGEX( ETC_Guest_Name__c , &quot;^[a-zA-Z0-9_]*$&quot;))
)</errorConditionFormula>
    <errorMessage>Special characters are not allowed, please correct.</errorMessage>
</ValidationRule>
