<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>preferred_start_date_validation_rule</fullName>
    <active>true</active>
    <errorConditionFormula>Preferred_Date__c &lt; NOW()</errorConditionFormula>
    <errorDisplayField>Preferred_Date__c</errorDisplayField>
    <errorMessage>Preferred start date should not be past date</errorMessage>
</ValidationRule>
