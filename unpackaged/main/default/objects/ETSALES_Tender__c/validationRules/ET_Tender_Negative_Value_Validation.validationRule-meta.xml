<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ET_Tender_Negative_Value_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>IF(ETSALES_Bid_Bond_Amount__c&lt; 0 ||  ETP_Amount__c &lt;0, TRUE, FALSE
)</errorConditionFormula>
