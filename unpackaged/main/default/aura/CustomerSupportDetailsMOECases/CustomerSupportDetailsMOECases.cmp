    <!-- Vinod Changes - Start --> 
    <div class="slds-m-around_xx-large">
        <aura:if  isTrue="{!v.confirmFlag}" > 
            <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open"> 
                <div class="slds-modal__container">
                    <header class="slds-modal__header">
                        <lightning:buttonIcon iconName="utility:close"
                                              onclick="{! c.closeConfirmModal }"
                                              alternativeText="close"
                                              variant="bare-inverse"
                                              class="slds-modal__close"/>
                        <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">{!v.clCaseApprovalProcess}</h2>
                    </header>               
                    <div class="slds-modal__content slds-p-around_medium bgImageDiv"  id="modal-content-id-1">
                        
                        <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">
                            {!v.clDOyouwantto}&nbsp;{!v.status}&nbsp;{!v.clthecase}</h2>
                        <lightning:textarea aura:id="comment" name="comments" label="{!v.clComments}" required="{!v.caseCommentsRequired}" messageWhenValueMissing="Complete this field." value="{!v.comments}" class="commetuser" style="text-align:center;"/>
                        
                    </div>
                    <footer class="slds-modal__footer">
                        <lightning:button variant="brand" 
                                          label="{!v.clCancel}"
                                          title="{!v.clCancel}"
                                          aura:id="status"
                                          onclick="{!c.closeConfirmModal}" class="btnsave"/>
                        <lightning:button variant="brand" 
                                          label="{!v.clSave}"
                                          title="{!v.clSave}"
                                          aura:id="status"
                                          disabled="{!v.isDisabled}"
                                          onclick="{!c.updateCase}" class="btnsave"/>
                    </footer>
                </div>
            </section> 
        </aura:if> 
    </div>
    <div class="footer_section">
