                                        </lightning:card>
                                    </aura:if>
                                </aura:if>
                            </aura:if> -->
                            
                            <aura:if isTrue="{!and(v.current_sequence eq 3,v.current_line.jobLineItem.Sequence__c eq 3)}">
                                <aura:if isTrue="{!and(!v.confirmHoldVar,!v.usageSection)}">
                                    <aura:if isTrue="{!and(!v.displayDefectSectionCutOff,!v.openToolsReqSection)}">
                                        <lightning:card  title="{!v.current_sequence+'. Rim Change Check'}"> 
                                            <div style="width:60%;margin:auto;margin-bottom:15px">
                                                <aura:if isTrue="{!and(not(v.step_error eq null),not(v.step_error eq ''))}">
                                                    <div class="slds-col slds-size_12-of-12" style="color:red">
                                                        <b>Error:&nbsp;</b>{!v.step_error}
                                                    </div>
                                                </aura:if>
                                                <aura:if isTrue="{!v.ChangeTyreRim}">
                                                	<!--p style="color:red">
                                                    	<b>WARNING: </b>As per system records, current rim size wont fit for newly taken job card tyre size.
                                                    </p-->
                                                    <aura:set attribute="else">
                                                        <p style="color:green">
                                                            <b>Note: </b>As per system records, current rim size can be reused for newly taken job card tyre size.
                                                        </p>
                                                    </aura:set>
                                                    <br/>
                                                    <div class="slds-form-element tool_Secondsec">
                                                        <b>Tyre Size:</b> {!v.current_line.relatedJobWrapper.validJobCardList[0].Tyre_Inventory__r.ETT_Tyre_Size_Master__r.Name}
                                                    </div>
                                                    <br/>
                                                </aura:if>
                                                <div class="slds-form-element tool_Secondsec">
                                                    <lightning:input type="checkbox" checked="{!v.current_line.jobLineItem.ETT_Verified_Building_Rim_Change__c}" label="Verified Building Rim Change &amp; taken action."/>
                                                </div>
                                            </div>  
                                        </lightning:card>
                                    </aura:if>
                                </aura:if>
                            </aura:if>
                            
                            <aura:if isTrue="{!and(v.current_sequence eq 4,v.current_line.jobLineItem.Sequence__c eq 4)}">
                                <aura:if isTrue="{!and(!v.confirmHoldVar,!v.usageSection)}">
                                    <aura:if isTrue="{!and(!v.displayDefectSectionCutOff,!v.openToolsReqSection)}">
                                        <lightning:card  title="{!v.current_sequence+'. Check Expandable Rim pressure'}"> 
                                            <div style="width:60%;margin:auto;margin-bottom:15px">
                                                <aura:if isTrue="{!and(not(v.step_error eq null),not(v.step_error eq ''))}">
                                                    <div class="slds-col slds-size_12-of-12" style="color:red">
                                                        <b>Error:&nbsp;</b>{!v.step_error}
                                                    </div>
                                                </aura:if>
                                                
                                                <div class="slds-form-element textboxdiv">
                                                    <label class="slds-form-element__label slds-text-title_bold" for="select-01">Initial Pressure met 25-30 psi?</label>
                                                    <div>
                                                        <lightning:select value="{!v.current_line.jobLineItem.ETT_Initial_Activity_State__c}" label="Initial Pressure met 25-30 psi?" variant="label-hidden"  disabled="{!v.current_line.jobLineItem.ETT_Status__c eq 'In-Progress'}">
                                                            <aura:iteration items="{!v.yesNoValues}" var="inv" indexVar="key">
                                                                <option text="{!inv.value}" value="{!inv.key}" selected="{!inv.key == v.current_line.jobLineItem.ETT_Initial_Activity_State__c}" />
                                                            </aura:iteration>
                                                        </lightning:select>
                                                    </div>
                                                </div>
                                                <aura:if isTrue="{!v.current_line.jobLineItem.ETT_Status__c eq 'In-Progress'}">
                                                    <div class="slds-form-element textboxdiv">
                                                        <label class="slds-form-element__label slds-text-title_bold" for="select-01">Final Pressure met 25-30 psi?</label>
                                                        <div>
                                                            <lightning:select value="{!v.current_line.jobLineItem.ETT_Final_Activity_State__c}" name="LightCheck" label="Final Pressure met 25-30 psi?" variant="label-hidden">
                                                                <aura:iteration items="{!v.noYesValues}" var="inv" indexVar="key">
                                                                <option text="{!inv.value}" value="{!inv.key}" selected="{!inv.key == v.current_line.jobLineItem.ETT_Final_Activity_State__c}" />
                                                            </aura:iteration>
                                                            </lightning:select>
                                                        </div>
                                                    </div>
                                                </aura:if>
                                            </div>  
                                        </lightning:card>
                                    </aura:if>
                                </aura:if>
                            </aura:if> 
                            
                            <aura:if isTrue="{!and(v.current_sequence eq 5,v.current_line.jobLineItem.Sequence__c eq 5)}">
                                <aura:if isTrue="{!and(!v.confirmHoldVar,!v.usageSection)}">
                                    <aura:if isTrue="{!and(!v.displayDefectSectionCutOff,!v.openToolsReqSection)}">
                                        <lightning:card  title="{!v.current_sequence+'. The Spacer/Rad Adjustment is ok ?'}"> 
                                            <div style="width:60%;margin:auto;margin-bottom:15px">
                                                <aura:if isTrue="{!and(not(v.step_error eq null),not(v.step_error eq ''))}">
                                                    <div class="slds-col slds-size_12-of-12" style="color:red">
                                                        <b>Error:&nbsp;</b>{!v.step_error}
                                                    </div>
                                                </aura:if>
                                                
                                                <div class="slds-form-element textboxdiv">
