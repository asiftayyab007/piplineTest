                            	<aura:if isTrue="{!and(!v.confirmHoldVar,!v.usageSection)}">
                                    <aura:if isTrue="{!and(!v.displayDefectSectionCutOff,!v.openToolsReqSection)}">
                                        <lightning:card  title="{!v.current_sequence+'. Check Floor Conditions'}"> 
