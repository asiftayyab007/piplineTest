                            <div aura:id="tyreLifeMoreThanOneMiscellaneousTabDataId" id="tab-default-5" class="slds-tabs--default__content slds-hide" role="tabpanel" aria-labelledby="color_linking">
                                <!-- step 2 and sub step E-->
                                <aura:if isTrue="{!v.SubStepNo eq '10'}">
                                    
                                    <div class="slds-form-element">
                                        <label class="slds-form-element__label slds-text-title_bold subheadingtag" for="select-01">E.	Miscellaneous</label>
                                        
                                    </div>
                                    <div class="slds-form-element textboxdiv">
                                        <label class="slds-form-element__label slds-text-title_bold" for="select-01">Good Condition</label>
                                        <div class="slds-select_container">
                                            <ui:inputSelect options="{!v.allMiscellaneousOptions.goodConditionsOption}"  aura:id="Miscellaneous" class="slds-select"  change="{!c.onBeadAreaChange}"/>
                                        </div>
                                    </div>
                                    
                                    <aura:if isTrue="{!v.allMiscellaneousOptions.selectedStatus eq 'No'}">
                                        
                                        <aura:iteration items="{!v.allMiscellaneousOptions.Miscellaneous}" var="item" indexVar="i">
                                            <div class="slds-form-element textboxdiv">
                                                <label class="slds-form-element__label slds-text-title_bold" for="select-01">{!item.label}</label>
                                                <div class="slds-select_container">
                                                    <lightning:select variant="label-hidden"  value="{!item.value}" class="label-hidden">
                                                        <aura:iteration items="{!item.options}" var="opt" indexVar="i">
                                                            <option value="{!opt.value}">{!opt.label}</option>
                                                        </aura:iteration>
                                                    </lightning:select>
                                                </div>
                                                <aura:if isTrue="{!item.value eq 'Rejected'}">
                                                    <div>
                                                        <div class="slds-grid slds-gutters slds-p-top_x-small">
                                                            <div class="slds-col slds-size_1-of-2">
                                                                <img src="{!item.imagelink}"/>
                                                                
                                                            </div>
                                                            <div class="slds-col slds-size_1-of-2">
                                                                <lightning:input height="200" width="350" 
                                                                                 label="File Upload"  
                                                                                 id="{!i}"  
                                                                                 files="{!v.fileToBeUploaded}"
                                                                                 type="file" 
                                                                                 name="Miscellaneous" 
                                                                                 multiple="false" 
                                                                                 required="false" 
                                                                                 onchange="{!c.handleFilesChange}"
                                                                                 />
                                                                <aura:iteration items="{!item.inputFields}" var="itemField">
                                                                    <aura:if isTrue="{!itemField.label == 'strFileName'}">
                                                                        {!itemField.value}
                                                                    </aura:if>
                                                                </aura:iteration>
                                                                
                                                            </div>
                                                        </div>
                                                        
                                                    </div>
                                                    <div>
                                                        <aura:iteration items="{!item.inputFields}" var="itemField">
                                                            <aura:if isTrue="{!itemField.label == 'Rejection Name' || itemField.label == 'Recommendation' || itemField.label == 'Cause Of Rejection' || itemField.label == 'Other'}">
                                                                <aura:if isTrue="{!itemField.label=='Other'}">
                                                                    <lightning:input title="{!itemField.value}" disabled="{!itemField.label != 'Other'}" required="false"  value="{!itemField.value}" label="{!itemField.label}" />                                                                    
                                                                    <aura:set attribute="else">
                                                                        <lightning:input title="{!itemField.value}" disabled="{!itemField.label != 'Other'}" required="true"  value="{!itemField.value}" label="{!itemField.label}" />                                                                    
                                                                    </aura:set>
                                                                </aura:if>
                                                                <!--
                                                                <lightning:input title="{!itemField.value}" disabled="{!itemField.label != 'Other'}" required="true"  value="{!itemField.value}" label="{!itemField.label}" />
-->                                                            
                                                            </aura:if>
                                                        </aura:iteration>
                                                    </div>
                                                </aura:if>
                                            </div>
                                        </aura:iteration>
                                    </aura:if>
                                </aura:if>
                            </div>  
                            
                            </aura:if>
                            
                        </div>
                    </div>  
                    <!-- Tyre Life 1 Stop -->
                <!--    <aura:set attribute="else"> -->
                <!--</aura:if> -->
                
                </aura:if>
                
            </div>
            
            
            <footer class="slds-modal__footer">
                <button class="slds-button slds-button_neutral" onclick="{!c.cancel}">Cancel</button>
                <!--<button disabled="{!v.SubStepNo eq '1'}" class="slds-button slds-button_brand" onclick="{!c.clickPrevious}">Previous</button>
                
                <button class="slds-button slds-button_brand" disabled="true">Submit</button>
                <button disabled="{!v.SubStepNo eq '5'}" class="slds-button slds-button_brand" onclick="{!c.clickNext}">Next</button>
                -->
                
                <aura:if isTrue="{!v.stepNo eq '1'}">
                    <!-- Tyre Life 1 Start -->
                    <!-- Selection of 1 -->
                    <aura:if isTrue="{!and(v.SubStepNo eq '1', v.selectedValue eq '1')}">
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.callSideWallArea}">Next</button>                                    
                    </aura:if>
                    <!-- Selection of B of step 1 -->
                    <aura:if isTrue="{!and(v.SubStepNo eq '2', v.selectedValue eq '1')}">
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.callBeadArea}">Previous</button>                                                                        
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.callCrownArea}">Next</button>                                                                        
                    </aura:if>  
                    <!-- Selection of C of step 1 -->
                    <aura:if isTrue="{!and(v.SubStepNo eq '3', v.selectedValue eq '1')}">
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.callSideWallArea}">Previous</button>                                                                        
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.callTyreInterior}">Next</button>                                                                        
                    </aura:if>
                    <aura:if isTrue="{!and(v.SubStepNo eq '4', v.selectedValue eq '1')}">
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.callCrownArea}">Previous</button>                                                                        
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.callAnyArea}">Next</button>                                                                        
                    </aura:if>
                    <aura:if isTrue="{!and(v.SubStepNo eq '5', v.selectedValue eq '1')}">
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.callTyreInterior}">Previous</button>                                                                        
                      
                        <button disabled="{!v.submitLifeOne}" class="slds-button slds-button_brand" onclick="{!c.SubmitTyreLifeOne}">Submit</button>                                                                        
                    </aura:if>
                    <!-- Tyre Life 1 Stop -->
                </aura:if>             
                
                <aura:if isTrue="{!or(v.stepNo eq '2',v.stepNo eq '3',v.stepNo eq '4',v.stepNo eq '5')}">
                    <!-- step 2 and sub step A-->
                    <aura:if isTrue="{!and(v.SubStepNo eq '1')}">
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.callSideWallArea}">Next</button>                                    
                    </aura:if>
                    <!-- Selection of B of step 1 -->
                    <aura:if isTrue="{!and(v.SubStepNo eq '2')}">
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.callBeadArea}">Previous</button>                                                                        
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.callCrownArea}">Next</button>                                                                        
                    </aura:if>
                    <!-- Selection of C of step 1 -->
                    <aura:if isTrue="{!and(v.SubStepNo eq '3')}">
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.callSideWallArea}">Previous</button>                                                                        
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.callTyreInterior}">Next</button>                                                                        
                    </aura:if>
                    <aura:if isTrue="{!and(v.SubStepNo eq '4')}">
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.callCrownArea}">Previous</button>                                                                        
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.callAnyArea}">Next</button>                                                                        
                    </aura:if>
                    
                    <aura:if isTrue="{!and(v.SubStepNo eq '5')}">
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.callTyreInterior}">Previous</button>                                                                        
                         <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.tyreLifeMoreThanOneHolesAndInjuries}">Next</button>
                        <!--  <button disabled="{!v.submitLifeOne}" class="slds-button slds-button_brand" onclick="{!c.SubmitTyreLifeOne}">Submit</button> -->                                                                       
                    </aura:if>
                    <aura:if isTrue="{!v.SubStepNo eq '6'}">
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.callAnyArea}">Previous</button>
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.tyreLifeMoreThanOneMissingLooseTread}">Next</button>
                    </aura:if>
                    <!-- step 2 and sub step B-->
                    <aura:if isTrue="{!v.SubStepNo eq '7'}">
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.tyreLifeMoreThanOneHolesAndInjuries}">Previous</button>
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.tyreLifeMoreThanOneCracks}">Next</button>
                    </aura:if>
                    <!-- step 2 and sub step C-->
                    <aura:if isTrue="{!v.SubStepNo eq '8'}">
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.tyreLifeMoreThanOneMissingLooseTread}">Previous</button>
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.tyreLifeMoreThanOneBulgesDepressions}">Next</button>
                    </aura:if> 
                    <!-- step 2 and sub step D-->
                    <aura:if isTrue="{!v.SubStepNo eq '9'}">
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.tyreLifeMoreThanOneCracks}">Previous</button>
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.tyreLifeMoreThanOneMiscellaneous}">Next</button>
                    </aura:if>
                    <!-- step 2 and sub step E-->
                    <aura:if isTrue="{!v.SubStepNo eq '10'}">
                        <button disabled="false" class="slds-button slds-button_brand" onclick="{!c.tyreLifeMoreThanOneBulgesDepressions}">Previous</button>
                        <button disabled="{!v.submitLifemultiple}" class="slds-button slds-button_brand" onclick="{!c.SubmitMoreThanOneTyreLife}">Submit</button>
                    </aura:if>
                </aura:if>
                
            </footer>
            
        </div>
        
    </section>
    <div class="slds-backdrop slds-backdrop_open"></div>
</aura:component>