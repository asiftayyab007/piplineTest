                                <div class="demo-only demo-only--sizing slds-grid slds-wrap">
                                    <div class="slds-size_1-of-2">
                                        <div class="slds-grid">
                                            <div class="slds-size_1-of-2">
                                                Number Of Accountant/Cashier
                                            </div>
                                            <aura:if isTrue="{!workforce.ET_Number_of_Accountants__c}">
                                                <div class="slds-size_1-of-2">
                                                    <b>{!workforce.ET_Number_of_Accountants__c}</b> </div>
                                            </aura:if>
                                        </div> 
                                    </div>
