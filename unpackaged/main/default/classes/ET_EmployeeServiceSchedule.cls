global class ET_EmployeeServiceSchedule implements Schedulable,Database.AllowsCallouts {
    
    public static ETEmployee.OutputType  employeeResult {get;set;}
    
    global void execute(SchedulableContext SC) {
        ETI_Batch_Employee_Service job = new ETI_Batch_Employee_Service('',null);
        Database.executeBatch(job,200);
    }
    
   //perform mapping for employee Master
   public static List<Account> doEmployeeMapping(List<ETEmployee.EmployeeData_element> employeeData){
      List<Account> employeesToInsert = new List<Account>();
      Id employee_recordTypeId =Schema.SObjectType.Account.getRecordTypeInfosByName().get('Employee').getRecordTypeId();
        
       if(employeeData!=null && employeeData.size() > 0){
           for(ETEmployee.EmployeeData_element record:employeeData){
               ETEmployee.EmployeeRecord_element row = record.EmployeeRecord;
               Account employee = new Account();
               if(row.EmployeeNumber!=null)
               employee.ETT_ERP_Number__c=row.EmployeeNumber;
               if(row.EmployeeNameEn!=null)
               employee.Name=row.EmployeeNameEn;
               if(row.EmployeeNameAr!=null)
               employee.ETIN_Name_Arabic__c=row.EmployeeNameAr;
               if(row.Location!=null)
               employee.BillingCity=row.Location;
               if(row.Department!=null)
               employee.ETIN_Department__c=row.Department;
               if(row.Designation!=null)
               employee.ETIN_Designation__c=row.Designation;
               if(row.SupervisorName!=null)
               employee.ETIN_Supervisor_Name__c=row.SupervisorName;
               if(row.OfficeNumber!=null)
               employee.ETIN_Office_Number__c=row.OfficeNumber;
               if(row.MobileNumber!=null)
               employee.ETIN_Mobile_Number__c=row.MobileNumber;
               employee.recordtypeId=employee_recordTypeId;               
               employeesToInsert.add(employee);
           }
       }
      return employeesToInsert;
   }
