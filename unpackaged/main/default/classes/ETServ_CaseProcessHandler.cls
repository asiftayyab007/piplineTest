public class ETServ_CaseProcessHandler {
    @InvocableMethod
    public static void caseProcess(List<Id> CaseIds){
        System.debug('CaseIds >>>  '+CaseIds);
        List<Case> caseList = New List<Case>();
        for(Case cs:[SELECT Id,OwnerId,Location__c,Location__r.Operations_Queue_Id__c,Account.Location__c FROM Case WHERE Id=:CaseIds]){
            if(cs.Account != null)
                cs.Location__c = cs.Account.Location__c;
            If(cs.Location__c != null && cs.Location__r.Operations_Queue_Id__c == null)
                cs.OwnerId = System.Label.ET_Services_Omni_Channel_Queue;
            else If(cs.Location__c != null && cs.Location__r.Operations_Queue_Id__c != null)
                cs.OwnerId = cs.Location__r.Operations_Queue_Id__c;
            caseList.add(cs);
        }
        Update caseList;
    }
