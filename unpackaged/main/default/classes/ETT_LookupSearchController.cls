                                   system.debug('sobjList '+sobjList);
                                      List<ResultWrapper> lstRet = new List<ResultWrapper>();
                                      Map<string, ETT_Inspection_Card__c> mapOfICReports = new Map<string, ETT_Inspection_Card__c>();
                                      for(WorkOrderLineItem s : sobjList){
                                          if(s.Inspection_Cards__r.size()>0){
                                              for(ETT_Inspection_Card__c ic : s.Inspection_Cards__r){
                                                  if(s.ETT_Tyre_Serial_Number__c == ic.ETT_Tyre_Serial_Number__c){
                                                      mapOfICReports.put(ic.ETT_Tyre_Serial_Number__c, ic);  
                                                  }
                                              }
                                          }
                                      }
                                      system.debug('mapOfICReports***'+mapOfICReports);
                                      Map<string, ETT_Job_Card__c> jobCards = new Map<string, ETT_Job_Card__c>();
                                      //Map<Id, ETT_Job_Card__c> jobCards = new Map<Id, ETT_Job_Card__c>([Select Id,Name,ETT_Inspection_Card__c FROM ETT_Job_Card__c WHERE ETT_Inspection_Card__r.ETT_Collection_Line_Item__c IN:sobjList]);
                                      for(ETT_Job_Card__c jobCard : [Select Id,Name,ETT_Inspection_Card__c,ETT_Inspection_Card__r.ETT_Tyre_Serial_Number__c FROM ETT_Job_Card__c WHERE ETT_Inspection_Card__r.ETT_Collection_Line_Item__c IN:sobjList]){
                                         jobCards.put(jobCard.ETT_Inspection_Card__r.ETT_Tyre_Serial_Number__c, jobCard);
