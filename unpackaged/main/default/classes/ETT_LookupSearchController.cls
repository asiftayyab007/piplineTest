                                      //String query = 'SELECT Id, ETT_Tyre_Serial_Number__c,ETT_Collection_Card__r.ETT_Accounts__c,ETT_Account__c,ETT_Collection_Card__c,ETT_Collection_Card__r.Name,ETT_Collection_Card__r.ETT_Service_Appointment__c,ETT_Collection_Card__r.ETT_Service_Appointment__r.AppointmentNumber,(Select Id,Name,ETT_Tyre_Serial_Number__c FROM Inspection_Cards__r) FROM WorkOrderLineItem WHERE ETT_Tyre_Serial_Number__c LIKE "%4321%"';
                                      
                                      
                                      system.debug('query '+query);
                                 /*  List<WorkOrderLineItem> delList = Database.query(query);
                                      
                                         for (WorkOrderLineItem s : delList){
