    @AuraEnabled
    public static string updateCase(string caseId,string TyreSerialNumber, string collectionCard,string inspectionCard,string jobCard, string accountId,String delivarynotenum,string tyreinspectioncard,String collectioncardlookup,String jobcardlookup,String conatctname){//, string serviceAppointmentId
         //String AccountId ;
         List<Case>  lstCase = [select Id,AccountId from Case where Id =:caseId];
        if(lstCase[0].AccountId !=accountId){
            throw new AuraHandledException('Account Names are different');
        }
        case cs =new Case(Id=caseId);
        cs.Tyre_Serial_Number__c = TyreSerialNumber;
        cs.Collection_Card__c = collectionCard;
        cs.Inspection_Card__c = inspectionCard;
        cs.Job_Card__c = jobCard;
      //  cs.Service_Appointment__c = serviceAppointmentId;
     //   system.debug('cs.Service_Appointment__c.accontId'+serviceAppointmentId.AccountId);
        cs.Delivery_Note_Number__c=delivarynotenum;
        cs.Tyre_Inspection_Card__c=tyreinspectioncard;
        cs.Collection_Card123__c=collectioncardlookup;
        cs.Job_Card123__c= jobcardlookup;
        cs.ContactId=conatctname;
        update cs;
        return '';
    }
    
