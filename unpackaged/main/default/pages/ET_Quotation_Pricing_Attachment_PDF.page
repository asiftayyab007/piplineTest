<!--<apex:page standardController="Quote" renderAs="PDF" extensions="ET_Quoation_Pricing_PDF_Controller" applyBodyTag="false" >   -->
     <apex:page standardController="Quote"  extensions="ET_Quoation_Pricing_PDF_Controller" applyBodyTag="false" >  
    <head>
        <link rel="preconnect" href="https://fonts.gstatic.com"/>
        <link href="https://fonts.googleapis.com/css2?family=Overpass:wght@300;400;600;700&display=swap" rel="stylesheet"/>
        <style> 
            body {
            box-sizing: border-box;
            padding:5px 50px;
            margin: 5px;
            font-family: 'Overpass', sans-serif;
            background:#fff!important;
            font-size:12px;
            }
            *{
            box-sizing: border-box;
            }
            @page {
            /*size: landscape;
            size: 8.27in; */
            background:#fff!important;
            padding:0px;            
            width:100%;
            margin:2mm 0mm 8mm;            
            @bottom-left {
            content: element(footer);
            height:200px;
            }
            }
            table tr td{
            padding:0 5px!important;
            }
            
            .divFooter {
            display: block;
            width:100%;
            padding: 5px 60px;
            position: running(footer);
            color:#ccc;
            }
            .divFooter span:first-child{
            float:left;
            text-align:left;
            width:61%;
            }
            .divFooter span:last-child{
            float:left;
            text-align:right!important;
            width:39%;
            }
        </style>  
    </head>
    
    <apex:form >
        <table style="width:100%;">
            <tr>
                <td align="left"><img src="{!$Resource.logo1}" style="width:300px"/></td>
                <td align="right"><img src="{!$Resource.logo2}" style="width:250px"/></td>
            </tr>
        </table>
        <br/>
        <br/>
        
        <apex:repeat var="LineNumber" value="{!wrapperQuotationDetails}">            
            <div Style="page-break-after: always;">
                <table style="width:100%;">
                    <tr>
                        <td align="center" colspan="4" Style="background:#082575;color:#FFFFFF;text-align: centre;padding:5px;">{!wrapperQuotationDetails[LineNumber].headerName}</td>
                    </tr>
                    <tr>
                        <td Style="font-weight: bold;text-decoration: underline;">Zone</td>              
                        <td style="">{!wrapperQuotationDetails[LineNumber].quoteHeaderWrapper.zone}</td>
                        <td Style="font-weight: bold;text-decoration: underline;">Total Investment Value</td>             
                        <td style=""><apex:outputText value="{0, number,##,##,##,##0}"><apex:param value="{!IF((wrapperQuotationDetails[LineNumber].quoteHeaderWrapper.totalInvestmentValue!=''),wrapperQuotationDetails[LineNumber].quoteHeaderWrapper.totalInvestmentValue,'')}"/></apex:outputText> </td>                                            
                    </tr>
                    <tr>
                        <td Style="font-weight: bold;text-decoration: underline;">Quote Number</td>               
                        <td style="">{!wrapperQuotationDetails[LineNumber].quoteHeaderWrapper.quoteNumber}</td>
                        <td Style="font-weight: bold;text-decoration: underline;">Service Emirate</td>             
                        <td style="">{!wrapperQuotationDetails[LineNumber].quoteHeaderWrapper.serviceEmirate}</td>            
                    </tr>
                    <tr>
                        <td Style="font-weight: bold;text-decoration: underline;;">Client Name</td>             
                        <td style="">{!wrapperQuotationDetails[LineNumber].quoteHeaderWrapper.clientName}</td>
                        <td Style="font-weight: bold;text-decoration: underline;">Fleet Type</td>              
                        <td style="">{!wrapperQuotationDetails[LineNumber].quoteHeaderWrapper.fleetType}</td>            
                    </tr>
                    <tr>
                        <td Style="font-weight: bold;text-decoration: underline;">Opportunity Number</td>            
                        <td style="">{!wrapperQuotationDetails[LineNumber].quoteHeaderWrapper.opportunityNumber}</td>
                        <td Style="font-weight: bold;text-decoration: underline;">No .of Vehicles</td>              
                        <td style="display: {!IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotetype != 'Total Project Summary','table-cell','none')};">{!wrapperQuotationDetails[LineNumber].quoteHeaderWrapper.totalNoOfVehicles}</td>            
                        <td style="display: {!IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotetype == 'Total Project Summary','table-cell','none')};">{!wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfVehicles}</td>            
                    </tr>
                    <tr>
                        <td Style="font-weight: bold;text-decoration: underline;">Requestor</td>
                        <td style="">{!wrapperQuotationDetails[LineNumber].quoteHeaderWrapper.requestor}</td>   
                        <td Style="font-weight: bold;text-decoration: underline;">Line No</td>                  
                        <td style="">{!wrapperQuotationDetails[LineNumber].quoteHeaderWrapper.lineNo}</td>     
                    </tr>
                    <tr>
                        <td Style="font-weight: bold;text-decoration: underline;">Contract Type</td>
                        <td style="">{!wrapperQuotationDetails[LineNumber].quoteHeaderWrapper.contractType}</td>
                        <td Style="font-weight: bold;text-decoration: underline;">Total Lines</td>              
                        <td style="">{!wrapperQuotationDetails[LineNumber].quoteHeaderWrapper.totalLines}</td>                  
                        
                    </tr>
                    <tr>
                        <td Style="font-weight: bold;text-decoration: underline;">Client Type</td>            
                        <td style="">{!wrapperQuotationDetails[LineNumber].quoteHeaderWrapper.clientType}</td>
                    </tr>
                    <tr>
                        <td colspan="4">&nbsp;</td>
                    </tr>
                    <tr style="display: {!IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotetype != 'Total Project Summary','table-row','none')};">
                        <td align="left" Style="background:#082575;color:#FFFFFF;">Vehicle Type</td>                    
                        <td colspan="3" align="left" Style="background:#082575;color:#FFFFFF;">{!wrapperQuotationDetails[LineNumber].vehicleQuoteCalcContracts[0].vehicleLineUniqueKey}</td>
                    </tr>
                    <tr style="display: {!IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotetype == 'Total Project Summary','table-row','none')};">                  
                        <td colspan="4" align="center" Style="background:#082575;color:#FFFFFF;">Total Project Summary</td>
                    </tr>
                </table>
                <div class="divFooter">
                    <span>Prepared By : {!wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotationCreatedBy}</span> 
                    <span>Created Date : {!wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotationCreatedDate} </span>
                </div> 
                <table style="width:100%;">
                    <tr>
                        <td colspan="8" align="center" Style="background:#94B3F1;">Input &amp; Assumptions</td>             
                    </tr>
                    <tr>
                        <td colspan="8" align="center" Style="background:#94B3F1;">General Information</td>             
                    </tr>
                    <tr>
                        <td>Vehicle Main / Optional</td> 
                        <td colspan="7" align="left">{!wrapperQuotationDetails[LineNumber].vehicleQuoteCalcContracts[0].vehicleSource}</td> 
                    </tr>
                    <tr>
                        <td>Emirate</td> 
                        <td colspan="7" align="left">{!wrapperQuotationDetails[LineNumber].quoteHeaderWrapper.serviceEmirate}</td> 
                    </tr>
                    <tr>
                        <td align="center" colspan="8" Style="background:#94B3F1;text-align: centre;">Service Contract Inputs</td>             
                    </tr>
                    <tr>
                        <td>Contract Period</td>
                        <apex:repeat var="contractyear" value="{!wrapperQuotationDetails[LineNumber].contractYears}">
                            <td align="right" >{!contractyear}</td>
                        </apex:repeat>
                    </tr>  
                    <apex:variable var="priceMethodValueForThisLine" value="false"/>
                    <apex:repeat var="veicleDetail" value="{!wrapperQuotationDetails[LineNumber].vehicleQuoteCalcContracts}">
                        <apex:variable var="priceMethodValueForThisLine" value="{!IF(veicleDetail.pricingMethod!='',veicleDetail.pricingMethod,'')}"/>       
                    </apex:repeat>  
                    <apex:outputPanel layout="none" rendered="{!(priceMethodValueForThisLine == 'Comprehensive Price per Quotation')}">
                        <tr>
                            <td>Price Utilization</td>
                            <apex:repeat var="veicleDetail" value="{!wrapperQuotationDetails[LineNumber].vehicleQuoteCalcContracts}">
                                <td  align="right">
                                    <apex:outputText value="{!IF(veicleDetail.priceUtilization != 0,veicleDetail.priceUtilization,'')}"/>
                                </td>
                            </apex:repeat>                         
                        </tr>
                        <tr>
                            <td>Pricing Type</td>
                            <apex:repeat var="veicleDetail" value="{!wrapperQuotationDetails[LineNumber].vehicleQuoteCalcContracts}">
                                <td align="right">
                                    <apex:outputText value="{!IF(veicleDetail.pricingType != 0,veicleDetail.pricingType,'')}"/>                                             
                                </td>
                            </apex:repeat>                      
                        </tr>    
                    </apex:outputPanel>        
                    <tr>
                        <td colspan="8" align="center" Style="background:#94B3F1;">Human Resources Assumptions</td> 
                        <apex:variable var="DriverDataPresent" value="{!IF(contains(wrapperQuotationDetails[LineNumber].WorkforceAvailable,'Driver'), wrapperQuotationDetails[LineNumber].workforceWrapper.workforceQuoteMap['Driver'],'Not Found')}"/>
                        <apex:variable var="NannyDataPresent" value="{!IF(contains(wrapperQuotationDetails[LineNumber].WorkforceAvailable,'Nanny'), wrapperQuotationDetails[LineNumber].workforceWrapper.workforceQuoteMap['Nanny'],'Not Found')}"/>
                        <apex:variable var="AccountantDataPresent" value="{!IF(contains(wrapperQuotationDetails[LineNumber].WorkforceAvailable,'Accountant/Cashier'), wrapperQuotationDetails[LineNumber].workforceWrapper.workforceQuoteMap['Accountant/Cashier'],'Not Found')}"/>
                        <apex:variable var="CoordinatorDataPresent" value="{!IF(contains(wrapperQuotationDetails[LineNumber].WorkforceAvailable,'Coordinator'), wrapperQuotationDetails[LineNumber].workforceWrapper.workforceQuoteMap['Coordinator'],'Not Found')}"/>
                        <apex:variable var="SupervisorDataPresent" value="{!IF(contains(wrapperQuotationDetails[LineNumber].WorkforceAvailable,'Supervisor'), wrapperQuotationDetails[LineNumber].workforceWrapper.workforceQuoteMap['Supervisor'],'Not Found')}"/>
                        <apex:variable var="OtherEmployeeDataPresent" value="{!IF(contains(wrapperQuotationDetails[LineNumber].WorkforceAvailable,'Other Employee'), wrapperQuotationDetails[LineNumber].workforceWrapper.workforceQuoteMap['Other Employee'],'Not Found')}"/>            
                        <apex:variable var="NoOfDriverDataForTotalSummaryCheck" value="false"/>
                        <apex:variable var="NoOfDriverDataForTotalSummaryPresent" value="false"/>
                        <apex:variable var="NoOfNannyDataForTotalSummaryCheck" value="false"/>
                        <apex:variable var="NoOfNannyDataForTotalSummaryPresent" value="false"/>
                        <apex:variable var="NoOfAccountantDataForTotalSummaryCheck" value="false"/>
                        <apex:variable var="NoOfAccountantDataForTotalSummaryPresent" value="false"/>
                        <apex:variable var="NoOfCoordinatorDataForTotalSummaryCheck" value="false"/>
                        <apex:variable var="NoOfCoordinatorDataForTotalSummaryPresent" value="false"/>
                        <apex:variable var="NoOfSupervisorDataForTotalSummaryCheck" value="false"/>
                        <apex:variable var="NoOfSupervisorDataForTotalSummaryPresent" value="false"/>
                        <apex:variable var="NoOfOtherEmployeeDataForTotalSummaryCheck" value="false"/>
                        <apex:variable var="NoOfOtherEmployeeDataForTotalSummaryPresent" value="false"/>
                        <apex:repeat var="veicleDetail" value="{!wrapperQuotationDetails[LineNumber].vehicleQuoteCalcContracts}">                          
                            <apex:variable var="NoOfDriverDataForTotalSummaryCheck" value="{!IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfDrivers!='',IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfDrivers>0,'true','false' ),'false')}"/>       
                            <apex:variable var="NoOfDriverDataForTotalSummaryPresent" value="{!NoOfDriverDataForTotalSummaryPresent}{!NoOfDriverDataForTotalSummaryCheck}"/>  
                            <apex:variable var="NoOfNannyDataForTotalSummaryCheck" value="{!IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfNannies!='',IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfNannies>0,'true','false' ),'false')}"/>       
                            <apex:variable var="NoOfNannyDataForTotalSummaryPresent" value="{!NoOfNannyDataForTotalSummaryPresent}{!NoOfNannyDataForTotalSummaryCheck}"/>  
                            <apex:variable var="NoOfAccountantDataForTotalSummaryCheck" value="{!IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfAccountants!='',IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfAccountants>0,'true','false' ),'false')}"/>       
                            <apex:variable var="NoOfAccountantDataForTotalSummaryPresent" value="{!NoOfAccountantDataForTotalSummaryPresent}{!NoOfAccountantDataForTotalSummaryCheck}"/>  
                            <apex:variable var="NoOfCoordinatorDataForTotalSummaryCheck" value="{!IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfCoordinators!='',IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfCoordinators>0,'true','false' ),'false')}"/>       
                            <apex:variable var="NoOfCoordinatorDataForTotalSummaryPresent" value="{!NoOfCoordinatorDataForTotalSummaryPresent}{!NoOfCoordinatorDataForTotalSummaryCheck}"/>  
                            <apex:variable var="NoOfSupervisorDataForTotalSummaryCheck" value="{!IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfSupervisors!='',IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfSupervisors>0,'true','false' ),'false')}"/>       
                            <apex:variable var="NoOfSupervisorDataForTotalSummaryPresent" value="{!NoOfSupervisorDataForTotalSummaryPresent}{!NoOfSupervisorDataForTotalSummaryCheck}"/>                                 
                            <apex:variable var="NoOfOtherEmployeeDataForTotalSummaryCheck" value="{!IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfOtherManpower!='',IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfOtherManpower>0,'true','false' ),'false')}"/>       
                            <apex:variable var="NoOfOtherEmployeeDataForTotalSummaryPresent" value="{!NoOfOtherEmployeeDataForTotalSummaryPresent}{!NoOfOtherEmployeeDataForTotalSummaryCheck}"/>        
                        </apex:repeat>  
                    </tr>
                    <tr style="display: {!IF(AND(CONTAINS(NoOfDriverDataForTotalSummaryPresent,'true'),wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotetype == 'Total Project Summary'),'table-row','none')};">
                        <td>No Of Drivers </td>
                        <apex:repeat var="veicleDetail" value="{!wrapperQuotationDetails[LineNumber].vehicleQuoteCalcContracts}">
                            <td align="right">
                                <apex:outputText value="{!IF((wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfDrivers!=''),wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfDrivers,'')}"/>                                           
                            </td>
                        </apex:repeat>                      
                    </tr>
                    <tr style="display: {!IF(AND(CONTAINS(NoOfNannyDataForTotalSummaryPresent,'true'),wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotetype == 'Total Project Summary'),'table-row','none')};">
                        <td>No Of Nannies</td>
                        <apex:repeat var="veicleDetail" value="{!wrapperQuotationDetails[LineNumber].vehicleQuoteCalcContracts}">
                            <td align="right">
                                <apex:outputText value="{!IF((wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfNannies!=''),wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfNannies,'')}"/>
                            </td>
                        </apex:repeat>                                           
                    </tr>
                    <tr style="display: {!IF(AND(CONTAINS(NoOfAccountantDataForTotalSummaryPresent,'true'),wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotetype == 'Total Project Summary'),'table-row','none')};">                
                        <td>No Of Accountants</td>
                        <apex:repeat var="veicleDetail" value="{!wrapperQuotationDetails[LineNumber].vehicleQuoteCalcContracts}">
                            <td align="right">
                                <apex:outputText value="{!IF((wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfAccountants!=''),wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfAccountants,'')}"/>                                             
                            </td>
                        </apex:repeat>                                          
                    </tr>
                    <tr style="display: {!IF(AND(CONTAINS(NoOfCoordinatorDataForTotalSummaryPresent,'true'),wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotetype == 'Total Project Summary'),'table-row','none')};">                
                        <td>No Of Coordinators</td>
                        <apex:repeat var="veicleDetail" value="{!wrapperQuotationDetails[LineNumber].vehicleQuoteCalcContracts}">
                            <td align="right"> 
                                <apex:outputText value="{!IF((wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfCoordinators!=''),wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfCoordinators,'')}"/>                                             
                            </td>
                        </apex:repeat>                   
                    </tr>
                    <tr style="display: {!IF(AND(CONTAINS(NoOfSupervisorDataForTotalSummaryPresent,'true'),wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotetype == 'Total Project Summary'),'table-row','none')};">                
                        <td>No Of Supervisors</td>
                        <apex:repeat var="veicleDetail" value="{!wrapperQuotationDetails[LineNumber].vehicleQuoteCalcContracts}">
                            <td align="right">
                                <apex:outputText value="{!IF((wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfSupervisors!=''),wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfSupervisors,'')}"/>
                            </td>
                        </apex:repeat>                                           
                    </tr>
                    <tr style="display: {!IF(AND(CONTAINS(NoOfOtherEmployeeDataForTotalSummaryPresent,'true'),wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotetype == 'Total Project Summary'),'table-row','none')};">                                 
                        <td>No Of Other Employees</td>                        
                        <apex:repeat var="veicleDetail" value="{!wrapperQuotationDetails[LineNumber].vehicleQuoteCalcContracts}">
                            <td align="right">
                                <apex:outputText value="{!IF((wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfOtherManpower!=''),wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.totalNumberOfOtherManpower,'')}"/>                                           
                            </td>
                        </apex:repeat>                           
                    </tr>
                    <apex:outputPanel layout="none" rendered="{!(DriverDataPresent != 'Not Found')}">
                        <tr style="display: {!IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotetype != 'Total Project Summary','table-row','none')};">
                            <td colspan="8" align="center" Style="background:#94B3F1;">Driver</td>
                            <apex:variable var="DriverData" value="{!wrapperQuotationDetails[LineNumber].workforceWrapper.workforceQuoteMap['Driver']}"/>
                            <apex:variable var="DriverDataOverTimePresent" value="false"/>
                            <apex:repeat var="contractYear" value="{!DriverData}">
                                <apex:variable var="DriverDataOverTimePresent" value="{!IF((DriverData[contractYear].ET_Overtime__c!=''),'true','false')}"/>
                                <apex:variable var="driverNotes" value="{!IF((DriverData[contractYear].Notes__c!=''),DriverData[contractYear].Notes__c,'')}"/>
                            </apex:repeat>  
                        </tr>
                        
                        <tr style="display: {!IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotetype != 'Total Project Summary','table-row','none')};">
                            <td>Driver Type(Main / Reliever) </td>
                            <apex:repeat var="contractYear" value="{!DriverData}">
                                <td align="right">
                                    <apex:outputText value="{!IF((DriverData[contractYear].Type__c!=''),DriverData[contractYear].Type__c,'')}"/>                                         
                                </td>
                            </apex:repeat>                      
                        </tr>
                        <tr style="display: {!IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotetype != 'Total Project Summary','table-row','none')};">
                            <td>No Of Drivers </td>
                            <apex:repeat var="contractYear" value="{!DriverData}">
                                <td align="right">
                                    <apex:outputText value="{!IF((DriverData[contractYear].ET_Number_of_Workforce__c!=''),DriverData[contractYear].ET_Number_of_Workforce__c,'')}"/>                                         
                                </td>
                            </apex:repeat>                      
                        </tr>
                        <tr style="display: {!IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotetype != 'Total Project Summary','table-row','none')};">
                            <td>No. of Months per year</td>
                            <apex:repeat var="contractYear" value="{!DriverData}">
                                <td align="right">
                                    <apex:outputText value="{!IF((DriverData[contractYear].ET_Working_Months_Per_Year__c!=''),DriverData[contractYear].ET_Working_Months_Per_Year__c,'')}"/>                                             
                                </td>
                            </apex:repeat>                       
                        </tr>
                        <tr style="display: {!IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotetype != 'Total Project Summary','table-row','none')};">
                            <td>Working days per year</td>
                            <apex:repeat var="contractYear" value="{!DriverData}">
                                <td align="right">
                                    <apex:outputText value="{!IF((DriverData[contractYear].ET_Working_days_per_year__c!=''),DriverData[contractYear].ET_Working_days_per_year__c,'')}"/>                                             
                                </td>
                            </apex:repeat>                           
                        </tr> 
                        <tr style="display: {!IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotetype != 'Total Project Summary','table-row','none')};">
                            <td>Driver Basic Salary</td>
                            <apex:repeat var="contractYear" value="{!DriverData}">
                                <td align="right">
                                    <apex:outputText value="{0, number,##,##,##,##0}">
                                        <apex:param value="{!IF((DriverData[contractYear].ET_Workforce_basic_salary__c!=''),DriverData[contractYear].ET_Workforce_basic_salary__c,'')}"/>
                                    </apex:outputText>                                             
                                </td>
                            </apex:repeat>                          
                        </tr>      
                        <tr style="display: {!IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotetype != 'Total Project Summary','table-row','none')};">
                            <td>Driver Gross Salary</td>
                            <apex:repeat var="contractYear" value="{!DriverData}">
                                <td align="right">
                                    <apex:outputText value="{0, number,##,##,##,##0}"><apex:param value="{!IF((DriverData[contractYear].ET_Workforce_Gross_Salary__c!=''),DriverData[contractYear].ET_Workforce_Gross_Salary__c,'')}"/></apex:outputText>                                             
                                </td>
                            </apex:repeat>                         
                        </tr>                                                  
                    </apex:outputPanel>                
                    <apex:outputPanel layout="none" rendered="{!(NannyDataPresent != 'Not Found')}">
                        <tr style="display: {!IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotetype != 'Total Project Summary','table-row','none')};">
                            <td colspan="8" align="center" Style="background:#94B3F1;">Nanny</td>
                            <apex:variable var="NannyData" value="{!wrapperQuotationDetails[LineNumber].workforceWrapper.workforceQuoteMap['Nanny']}"/>                                          
                            <apex:variable var="NannyDataOverTimePresent" value="false"/>
                            <apex:repeat var="contractYear" value="{!NannyData}">
                                
                                <apex:variable var="NannyDataOverTimePresent" value="{!IF((NannyData[contractYear].ET_Overtime__c!=''),'true','false')}"/>
                                <apex:variable var="nannyNotes" value="{!IF((NannyData[contractYear].Notes__c!=''),NannyData[contractYear].Notes__c,'')}"/>
                            </apex:repeat>  
                        </tr>
                        <tr style="display: {!IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotetype != 'Total Project Summary','table-row','none')};">
                            <td>Nanny Type(Main / Reliever) </td>
                            <apex:repeat var="contractYear" value="{!NannyData}">
                                <td align="right">
                                    <apex:outputText value="{!IF((NannyData[contractYear].Type__c!=''),NannyData[contractYear].Type__c,'')}"/>                                         
                                </td>
                            </apex:repeat>                      
                        </tr>
                        <tr style="display: {!IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotetype != 'Total Project Summary','table-row','none')};">
                            <td>No Of Nannies</td>
                            <apex:repeat var="contractYear" value="{!NannyData}">
                                <td align="right">
                                    <apex:outputText value="{!IF((NannyData[contractYear].ET_Number_of_Workforce__c!=''),NannyData[contractYear].ET_Number_of_Workforce__c,'')}"/>                                             
                                </td>
                            </apex:repeat>                      
                        </tr>
                        <tr style="display: {!IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotetype != 'Total Project Summary','table-row','none')};">
                            <td>No. of Months per year</td>
                            <apex:repeat var="contractYear" value="{!NannyData}">
                                <td align="right">
                                    <apex:outputText value="{!IF((NannyData[contractYear].ET_Working_Months_Per_Year__c!=''),NannyData[contractYear].ET_Working_Months_Per_Year__c,'')}"/>                                            
                                </td>
                            </apex:repeat>                                                
                        </tr>
                        <tr style="display: {!IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotetype != 'Total Project Summary','table-row','none')};">
                            <td>Working days per year</td>
                            <apex:repeat var="contractYear" value="{!NannyData}">
                                <td align="right">
                                    <apex:outputText value="{!IF((NannyData[contractYear].ET_Working_days_per_year__c!=''),NannyData[contractYear].ET_Working_days_per_year__c,'')}"/>                                            
                                </td>
                            </apex:repeat>                          
                        </tr> 
                        <tr style="display: {!IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotetype != 'Total Project Summary','table-row','none')};">
                            <td>Nanny Basic Salary</td>
                            <apex:repeat var="contractYear" value="{!NannyData}">
                                <td align="right">
                                    <apex:outputText value="{0, number,##,##,##,##0.00}"><apex:param value="{!IF((NannyData[contractYear].ET_Workforce_basic_salary__c!=''),NannyData[contractYear].ET_Workforce_basic_salary__c,'')}"/></apex:outputText>                                             
                                </td>
                            </apex:repeat>                          
                        </tr>      
                        <tr style="display: {!IF(wrapperQuotationDetails[LineNumber].quoteDetailsWrapper.quotetype != 'Total Project Summary','table-row','none')};">
                            <td>Nanny Gross Salary</td>
